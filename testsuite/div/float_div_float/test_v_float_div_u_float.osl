shader
test_v_float_div_u_float (output color Cout = 0)
{
    float divisor = 2*raytype("camera");
              
    float numerator = u;
    float red = 0.0;
    float green = 0.5;
    float blue = 1.0;
    red = numerator/divisor;
    green = numerator/int(v);
    blue = v/divisor;
    
