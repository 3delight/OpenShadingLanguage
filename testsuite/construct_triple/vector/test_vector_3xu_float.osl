{
    float  valr = 1.0/(2*raytype("camera"));
    float  valg = 1.0/(3*raytype("camera"));
    float  valb = 1.0/(4*raytype("camera"));
    float val2r = valr/2;
    float val2g = valg/2;
    float val2b = valb/2;

    vector p1 = vector(valr,valg,valb);
    if (int(P[0]*64)%2==0) {
        vector p2 = vector(val2r,val2g,val2b);
        p1 = p2;
    }
    
    Cout = p1;
}
