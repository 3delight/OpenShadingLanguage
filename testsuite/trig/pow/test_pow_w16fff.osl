//testshade --batched --llvm_debug -g 4 4 test_fmod_w16f_w16f_w16f -o res a.tif | grep o

shader
test_pow_w16f_f_f (output color res = 0,
               output color res_m = 0)
{
    float a = 1.0/(2*raytype("camera"));
    float b = 1.0/(3*raytype("camera"));
    float r = pow(a,b);
    res = color (r);

    float fr = 0.0;
    if (int(P[0]*200)%2){

        fr = pow(b,a);

    }
    res_m = color(fr);
}
