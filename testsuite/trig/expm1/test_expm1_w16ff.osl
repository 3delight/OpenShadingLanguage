shader
test_expm1_w16f_f (output color res = 0,
               output color res_m = 0)
{
    float a = 1.0/(2*raytype("camera"));
    float b = 1.0/(3*raytype("camera"));
    float r = expm1(a);
    res = color (r);

    float fr = 0.0;
    if (int(P[0]*200)%2){

        fr = expm1(b);

    }
    res_m = color(fr);
    res = color (r);
}